.316406 A 1.0001 1.0001 0 0 0 19 23 L 22 23 A 1.0001 1.0001 0 0 0 22.970703 22.242188 L 23.78125 19 z M 17.732422 27 L 32.267578 27 L 32.085938 29 L 17.914062 29 L 17.732422 27 z"></path>
                </svg>

            </a>
            <button class="navbar-toggler border-0" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation" style="outline:none;box-shadow: none;">
                <i class="bi bi-list fs-1"></i>
            </button>

            <div class="collapse navbar-collapse justify-content-end me-lg-5" id="navbarSupportedContent">
                <ul class="navbar-nav mb-2 mb-lg-0 align-items-center">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="/">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#linksfooter">Links</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <div class="container pt-5 pb-5 px-md-4 col-12 col-md-8">
        <div class="d-flex justify-content-between h-75 border p-3 pt-sm-4 bg-white rounded-top">
            <div class="container-fluid p-0" id="up">
                <div class="d-flex">
                    <h2>
                        <%= dadosUser.name%>
                    </h2>
                </div>
                <p class="mb-0">
                    <%= dadosUser.tag%>
                </p>

                <div class="py-1">
                    <img src="https://cdn.royaleapi.com/cdn-cgi/image/w=20,h=26,format=auto/static/img/ui/trophy.png?">
                    <span style="font-size: 15px;"><%= dadosUser.trophies %> / <%= dadosUser.bestTrophies %> <%= dadosUser.arena.name %></span> 
                    <br>

                    <% if(dadosUser.currentPathOfLegendSeasonResult.trophies){ %>
                        <img src="https://cdns3.royaleapi.com/cdn-cgi/image/w=20,h=40,format=auto/static/img/ui/global-tournament.png">
                        <span class="mt-5"><%= dadosUser.currentPathOfLegendSeasonResult.trophies %></span>    
                    <% } %>     
                </div>

                <% if (dadosUser.clan && dadosUser.clan.name != undefined) { %>
                    <h6>
                        <%= dadosUser.clan.name %> | <%=  dadosUser.role %>
                    </h6>
                <% } %>
            </div>
            
            <div>
                <% switch (dadosUser.currentPathOfLegendSeasonResult.leagueNumber) { 
                    case 1: %>
                       <img src="https://cdn.royaleapi.com/static/img/arenas-fs8/128x128/league1-fs8.png?t=87d3ea2fc?2" class="img-fluid" alt="ícone arena 1">
                    <% break; %>
                    <% case 2: %>
                       <img src="https://cdn.royaleapi.com/static/img/arenas-fs8/128x128/league2-fs8.png?t=95a10fabc?2" class="img-fluid" alt="ícone arena 2" >
                    <% break; %>
                    <% case 3: %>
                       <img src="https://cdn.royaleapi.com/static/img/arenas-fs8/128x128/league3-fs8.png?t=7ce95892c?2" class="img-fluid" alt="ícone arena 3">
                    <% break; %>
                    <% case 4: %>
                        <img src="https://cdn.royaleapi.com/static/img/arenas-fs8/128x128/league4-fs8.png?t=3799a5c0c?2" class="img-fluid" alt="ícone arena 4">
                    <% break; %>
                    <% case 5: %>
                       <img src="https://cdn.royaleapi.com/static/img/arenas-fs8/128x128/league5-fs8.png?t=afb11b54c?2" class="img-fluid" alt="">
                    <% break; %>
                    <% case 6: %>
                       <img src="https://cdn.royaleapi.com/static/img/arenas-fs8/128x128/league6-fs8.png?t=02e05c3fc?2" class="img-fluid" alt="">
                    <% break; %>

                    <% case 7: %>
                       <img src="https://cdn.royaleapi.com/static/img/arenas-fs8/128x128/league7-fs8.png?t=b950556ac?2" class="img-fluid" alt="">
                    <% break; %>

                    <% case 8: %>
                       <img src="https://cdn.royaleapi.com/static/img/arenas-fs8/128x128/league8-fs8.png?t=971bd02cc?2" class="img-fluid" alt="">
                    <% break; %>
                    
                    <% case 9: %>
                       <img src="https://cdn.royaleapi.com/static/img/arenas-fs8/128x128/league9-fs8.png?t=a34707e7c?2" class="img-fluid" alt="">
                    <% break; %>

                    <% case 10: %>
                       <img src="https://cdn.royaleapi.com/static/img/arenas-fs8/128x128/league10-fs8.png?t=ccdd529fc?2" class="img-fluid" alt="">
                    <% break;
                } %>
            </div>
        </div>

        <div class="border border-body-secondary bg-body-secondary d-flex p-0">
            <a class="text-decoration-none text-dark border border-0 border-body-secondary p-1 px-3" href="/jogador?tag=<%=tag%>"><i class="bi bi-house-fill"></i></a>
            <a class="text-decoration-none text-dark border border-2 border-body-secondary border-top-0 border-bottom-0 p-1 px-3" href="/jogador/batalhas?tag=<%= tag %>&pag=1">Batalhas</a>
        </div>

        <div class="bg-white ">
            <%dadosPaginado.forEach(function(element){ %>
                
                <div class="p-2 text-center align-items-center border border-danger">
                    
                    <h5><%=element.gameMode.name.replace(/Ranked1v1_/, '').replace(/([a-z])([A-Z])/g, '$1 $2')%></h5>

                    <%element.team.forEach(function(elementTeam){ %>

                        <%element.opponent.forEach(function(elementOpponent){ %>

                            <div id="resultado" class="d-flex justify-content-center">
                                <h5><%=elementTeam.crowns%></h5>-<h5><%=elementOpponent.crowns%></h5>
                            </div>

                            
                            <div class="container d-flex ">
                                
                                <!-- infos player -->
                                <div id="team" class="col-6" style="font-size: 13px;">
                                    <div class="fw-bold"><%=elementTeam.name%></div>

                                    <%if(elementTeam.clan){ %>
                                        <span><%=elementTeam.clan.name%></span>
                                    <%}else{%>
                                        <span>no clan</span>
                                    <%}%>

                                    <div>
                                        <%if(elementTeam.globalRank != null){ %>
                                            <span>Rank#<%=elementTeam.globalRank%></span>
                                        <% } %>
                                    </div>

                                    <div class="d-flex flex-wrap pe-2 pe-md-5 p-1">
                                        <%elementTeam.cards.forEach(function(e){ %>
                                            
                                            <img src="<%= e.iconUrls.medium%>" class="w-25 object-fit-contain">
                                        <% }) %>
                                    </div>

                                </div>

                                <!-- infos oponente -->
                                <div id="opponent" class="col-6"  style="font-size: 13px;">
                                    <div class="fw-bold"><%=elementOpponent.name%></div>

                                    <%if(elementOpponent.clan){ %>
                                        <span><%=elementOpponent.clan.name%></span>
                                    <%}else{%>
                                        <span>no clan</span>
                                    <%}%>
                                    
                                    <div>
                                        <%if(elementOpponent.globalRank != null){ %>
                                            <span>Rank#<%=elementOpponent.globalRank%></span>
                                        <% } %>
                                    </div>

                                    <div class="d-flex flex-wrap ps-2 ps-md-5 p-1">
                                        <%elementOpponent.cards.forEach(function(e){ %>
                                            
                                            <img src="<%= e.iconUrls.medium%>" class="w-25 object-fit-contain">
                                        <% }) %>
                                    </div>
                                </div>

                            </div>

                        <%})%>
                    <%})%>
                </div>
            <%})%>
        </div>
    </div>
    
    <nav aria-label="Page navigation example" class="bg-light">
        <ul class="pagination justify-content-center mb-0">
            <li><a class="page-link" href="/jogador/batalhas?tag=<%=tag%>&pag=1">1</a></li>
            <li><a class="page-link" href="/jogador/batalhas?tag=<%=tag%>&pag=2">2</a></li>
        </ul>
    </nav>
    
    <footer class="text-center bg-dark text-white pt-3 w-100 " id="linksfooter">
        <h5>Social</h5>
        <div class="p-2">
            <a href="https://www.linkedin.com/in/luiz-henrique-78a207230/" class="px-3 text-white"><i
                    class="bi bi-linkedin"></i></a>
            <a href="https://github.com/luizhenrq28/clashroyalestats" class="px-3 text-white"><i
                    class="bi bi-github"></i></a>
            <a href="https://www.instagram.com/luizhenrq28/" class="px-3 text-white"><i class="bi bi-instagram"></i></a>
        </div>
        <hr>
        <p class="container col-9 align-items-center">Todo o conteúdo relacionado à Supercell disponibilizado neste sistema
            segue a Política de Conteúdo de Fãs da Supercell, garantindo respeito aos direitos autorais e às diretrizes de
            uso de marca e conteúdo. <a class="text-white" target="_blank"
                href="https://supercell.com/en/fan-content-policy/pt/"><i class="bi bi-box-arrow-in-up-right"></i></a></p>
    </footer>

</body>
</html>